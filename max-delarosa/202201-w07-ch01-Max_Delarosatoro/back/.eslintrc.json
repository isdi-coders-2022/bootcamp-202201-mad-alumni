{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "extends": ["airbnb", "prettier"],
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "no-underscore-dangle": "off"
    }
}
